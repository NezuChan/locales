{
  "name": "@nezuchan/locales",
  "version": "2.0.0",
  "description": "A Public Locales Translation Repository for NezukoChan.",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint src --ext ts",
    "lint:fix": "eslint src --fix --ext ts",
    "build": "swc src -d dist && tsc --emitDeclarationOnly"
  },
  "type": "module",
  "author": "KagChi",
  "license": "GPL-3.0",
  "devDependencies": {
    "@discordjs/builders": "^1.5.0",
    "@hazmi35/eslint-config": "^8.7.1",
    "@nezuchan/core": "^0.3.8",
    "@nezuchan/decorators": "^0.1.0",
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.40",
    "@types/amqplib": "^0.10.1",
    "@types/node": "^18.15.2",
    "@typescript-eslint/eslint-plugin": "^5.53.0",
    "@typescript-eslint/parser": "^5.53.0",
    "eslint": "^8.34.0",
    "rimraf": "^4.4.0",
    "typescript": "^4.9.5"
  },
  "eslintConfig": {
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint"
    ],
    "parserOptions": {
      "project": "./tsconfig.json"
    },
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended",
      "@hazmi35/eslint-config/typescript"
    ],
    "rules": {
      "class-methods-use-this": "off",
      "@typescript-eslint/naming-convention": "off"
    }
  },
  "dependencies": {
    "@nezuchan/i18n": "^2.0.0"
  }
}
